<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:Travel_Company.WPF.MVVM.View"
                    xmlns:employeesView="clr-namespace:Travel_Company.WPF.MVVM.View.Employees"
                    xmlns:clientsView="clr-namespace:Travel_Company.WPF.MVVM.View.Clients"
                    xmlns:viewModels="clr-namespace:Travel_Company.WPF.MVVM.ViewModel"
                    xmlns:employeesVM="clr-namespace:Travel_Company.WPF.MVVM.ViewModel.Employees"
                    xmlns:clientsVM="clr-namespace:Travel_Company.WPF.MVVM.ViewModel.Clients">
    <DataTemplate DataType="{x:Type TypeName=viewModels:LoginViewModel}">
        <views:LoginView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type TypeName=employeesVM:EmployeesViewModel}">
        <employeesView:EmployeesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=employeesVM:EmployeesUpdateViewModel}">
        <employeesView:EmployeesUpdateView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=employeesVM:EmployeesCreateViewModel}">
        <employeesView:EmployeesCreateView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=clientsVM:ClientsViewModel}">
        <clientsView:ClientsView />
    </DataTemplate>
</ResourceDictionary>